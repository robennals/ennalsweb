{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/rob.html",
        "destination": "https://robennals.org",
        "type": 301
      },
      {
        "source": "/rob",
        "destination": "https://robennals.org",
        "type": 301
      },
      {
        "source": "/rob/index.html",
        "destination": "https://robennals.org",
        "type": 301
      }
    ]
  }
}
